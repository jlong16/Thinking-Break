{
  "manifest_version": 3,
  "name": "Thinking Break",
  "version": "1.0",
  "description": "Sends a pop-up after 1 minute.",
  "permissions": [
    "alarms",
    "notifications",
    "background",
    "storage",
    "activeTab",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
